#: validators.py:31
#, python-format
msgid "Invalid Length (%s)"
msgstr "Tamanho Inválido: %s"

#: validators.py:41
#, python-format
msgid "Must be %s characters or more"
msgstr "Deve ser %s carácter(es) ou mais"

#: validators.py:45
#, python-format
msgid "Must be %s characters or less"
msgstr "Deve ser %s carácter(es) ou menos"

#: validators.py:49
#, python-format
msgid "Must be more complex (%s)"
msgstr "Deve ser mais complexa (%s)"

#: validators.py:77
#, python-format
msgid "Must contain %(UPPER)s or more unique uppercase characters"
msgstr "Deve conter %(UPPER)s ou carácteres maiúsculos mais únicos"

#: validators.py:81
#, python-format
msgid "Must contain %(LOWER)s or more unique lowercase characters"
msgstr "Deve conter %(LOWER)s ou carácteres minúsculos mais únicos"

#: validators.py:85
#, python-format
msgid "Must contain %(DIGITS)s or more unique digits"
msgstr "Deve conter %(DIGITS)s ou dígitos mais únicos"

#: validators.py:89
#, python-format
msgid "Must contain %(PUNCTUATION)s or more unique punctuation characters"
msgstr "Deve conter %(PUNCTUATION)s ou carácteres de pontuação mais únicos"

#: validators.py:93
#, python-format
msgid "Must contain %(SPECIAL)s or more unique special characters"
msgstr "Deve conter %(SPECIAL)s ou carácteres especiais mais únicos"

#: validators.py:97
#, python-format
msgid "Must contain %(WORDS)s or more unique words"
msgstr "Deve conter %(WORDS)s ou palavras mais únicas"

#: validators.py:102
#, python-format
msgid "Too Similar to [%(haystacks)s]"
msgstr "Muito Similar a [%(haystacks)s]"

#: validators.py:138
msgid "Based on a dictionary word"
msgstr "Baseado numa palavra de dicionário"

#: validators.py:154
msgid "Based on a common sequence of characters"
msgstr "Baseado em uma sequência comum de carácteres"
